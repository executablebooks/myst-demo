    "eslint-plugin-import": "^2.23.3",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.1",
    "lit-analyzer": "^1.1.10",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.2",
    "typescript": "^3.8.3",
    "web-component-analyzer": "^1.0.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
